{
  "name": "@litekart/api",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "description": "Litekart API",
  "author": "Swadesh Behera<swadesh@litekart.in>",
  "scripts": {
    "predev": "rimraf dist",
    "seed": "ts-node-dev --respawn --transpile-only src",
    "dev": "ts-node-dev --transpile-only --respawn src",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.prod.json",
    "lived": "run-s docker-build docker-push",
    "up": "docker-compose -f docker-compose.dev.yml up",
    "down": "docker-compose -f docker-compose.dev.yml down",
    "docker-build": "docker build -t itswadesh/litekart .",
    "next": "node devops/next",
    "edeal": "node devops/edeal",
    "anne": "node devops/anne",
    "misiki": "node devops/misiki",
    "misiki.io": "node devops/misiki.io",
    "mywondermall": "node devops/mywondermall",
    "gro": "node devops/gro",
    "copy": "node devops/copy",
    "copy-mywondermall": "node devops/mywondermall/copy",
    "copy-anne": "node devops/anne/copy",
    "copy-edeal": "node devops/edeal/copy",
    "copy-misiki.io": "node devops/misiki.io/copy",
    "copy-misiki": "node devops/misiki/copy",
    "prod": "run-s build copy",
    "live": "run-s build copy-misiki misiki copy-misiki.io misiki.io copy-edeal edeal copy-anne anne copy-mywondermall mywondermall",
    "start": "cross-env NODE_ENV=production node dist/src/index.js",
    "test": "jest",
    "lint": "eslint --ext .ts --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "prepush": "npm run lint",
    "conventional-changelog": "conventional-changelog",
    "release:changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
    "ts-prettify": "./node_modules/.bin/tsfmt -r --baseDir \"./src\"",
    "prettify": "prettier --write \"./src\"",
    "pull": "git submodule update --init --recursive",
    "docker-push": "docker push itswadesh/litekart",
    "acr-login": "az acr login --name tblzstgacr",
    "acr": "az acr build -r tblzstgacr . -t tblzstgacr.azurecr.io/tablez-backend:latest"
  },
  "homepage": "https://litekart.in",
  "repository": {
    "type": "git",
    "url": "https://github.com/itswadesh/svelte-commerce"
